    "desktop:main:dev": "nodemon -w ./desktop/src --exec electron ./desktop/src/main.js",
    "desktop:dev": "concurrently \"npm run desktop:client:build:watch\" \"npm run desktop:main:dev\"",
    "cli:start": "node ./cli/index.js",
    "lint": "eslint . --ext .js",
    "e2e": "node ./e2e/index.js",
